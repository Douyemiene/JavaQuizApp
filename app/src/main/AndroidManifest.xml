<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.javaquizapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionOne"
            android:label="@string/one" />
        <activity
            android:name=".QuestionTwo"
            android:label="@string/two" />
        <activity
            android:name=".QuestionThree"
            android:label="@string/three" />
        <activity
            android:name=".QuestionFour"
            android:label="@string/four" />
        <activity
            android:name=".QuestionFive"
            android:label="@string/five" />
        <activity android:name=".DisplayScore"></activity>
    </application>

</manifest>